#myTopnav.topnav
  a.active(href='/') Home
  a.title_bar(href='/misinformationdemo') Demo
  a(href='http://tamu.edu/' style='float:   right') Texas A&amp;M University
  a.icon(href='javascript:void(0);' style='font-size:15px;' onclick='myFunction()') &#x2630;
#chartDiv
  #xai
    h3#explaination_title
      | Please highlight any words realated to &quot;topic &quot; news in this article: (1/14)
    #annotation_area
    #control
      h1 Attention Explanation:
      label.radioBut
        | Word-level
        input(type='radio' checked='checked' name='radio')
        span.radiomark
      label.radioBut
        | Phrase-level
        input(type='radio' name='radio')
        span.radiomark
      h1 Other Explanation:
      label.checkBut
        | Linguistic Features
        input(type='checkbox' checked='checked')
        span.checkmark
      label.checkBut
        | Knowledge Graph
        input(type='checkbox')
        span.checkmark
    #prediction
      #gauge_top_note
      canvas#the_gauge
      #gauge_note
    #submit
      #nav_tweets
        button.nav_button(onclick='lastArticle()') Previous Tweet
        button.nav_button(onclick='nextArticle()') Next Tweet
      #annotate_tweets
        button.submit_button(onclick='removeHighlights()') Clear
        button.submit_button(onclick='nextArticle()') Submit
  #related_articles
    .choice_buttons
      h1 Related Articles:
      button(onclick='showOne()') Article 1
      button(onclick='showTwo()') Article 2
      button(onclick='showThree()') Article 3
    #article_area
      h1 The article:
  #last_box
